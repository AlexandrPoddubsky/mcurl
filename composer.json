{
    "name": "khr/php-mcurl-client",
    "description": "wrap curl client for PHP 5.3; using multi curl, parallel request and write asynchronous code",
    "keywords": ["php", "curl","multi", "client"],
    "homepage": "http://github.com/khristenkoyura/mcurl",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Khristenko Yura",
            "email": "khristenkoyura@gmail.com"
        }
    ],
    "repositories" : [{
        "type" : "package",
        "package"  : {
            "name"    : "libraries/phpquery",
            "version" : "0.9.5",
            "dist"  : {
                "type"  : "zip",
                "url"  : "http://phpquery.googlecode.com/files/phpQuery-0.9.5.386.zip"
            },
            "autoload": {
                "classmap"  : ["phpQuery/phpQuery.php"]
            }
        }
    }],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "libraries/phpquery": "*"
    },
    "autoload": {
        "psr-4": {"MCurl\\": "src/"}
    }
}